{
  "$schema": "../../schemas/test-file.json",
  "name": "US Secondary Units",
  "description": "US address parsing for secondary unit types including PO boxes, apartment designations, and facility addresses with varied formatting",
  "tests": {
    "unitsAndBoxes": [
      {
        "input": "Post office Box 3094 Collierville TN 38027",
        "expected": {
          "secUnitType": "PO Box",
          "secUnitNum": "3094",
          "city": "Collierville",
          "state": "TN",
          "zip": "38027"
        },
        "description": "Full 'Post office Box' format"
      },
      {
        "input": "P.O. box 3094 Collierville TN 38027",
        "expected": {
          "secUnitType": "PO Box",
          "secUnitNum": "3094",
          "city": "Collierville",
          "state": "TN",
          "zip": "38027"
        },
        "description": "Abbreviated 'P.O. box' format with periods"
      },
      {
        "input": "POBox 3094 Collierville TN 38027",
        "expected": {
          "secUnitType": "PO Box",
          "secUnitNum": "3094",
          "city": "Collierville",
          "state": "TN",
          "zip": "38027"
        },
        "description": "Compact 'POBox' format without spaces"
      },
      {
        "input": "#42 233 S Wacker Dr 60606",
        "expected": {
          "secUnitType": "#",
          "secUnitNum": "42",
          "number": "233",
          "prefix": "S",
          "street": "Wacker",
          "type": "Dr",
          "zip": "60606"
        },
        "description": "Hash symbol unit prefix format"
      },
      {
        "input": "lt42 99 Some Road, Some City LA",
        "expected": {
          "secUnitType": "Lot",
          "secUnitNum": "42",
          "number": "99",
          "street": "Some",
          "type": "Rd",
          "city": "Some City",
          "state": "LA"
        },
        "description": "Lot number unit designation"
      },
      {
        "input": "233 S Wacker Dr lobby 60606",
        "expected": {
          "number": "233",
          "prefix": "S",
          "street": "Wacker",
          "type": "Dr",
          "secUnitType": "Lobby",
          "zip": "60606"
        },
        "description": "Lobby as secondary unit designation"
      },
      {
        "input": "(233 S Wacker Dr lobby 60606)",
        "expected": {
          "number": "233",
          "prefix": "S",
          "street": "Wacker",
          "type": "Dr",
          "secUnitType": "Lobby",
          "zip": "60606"
        },
        "description": "Address enclosed in parentheses"
      },
      {
        "input": "789 Pine St Apt 4B, Seattle, WA 98101",
        "expected": {
          "number": "789",
          "street": "Pine",
          "type": "St",
          "unit": "Apt 4B",
          "secUnitType": "Apartment",
          "secUnitNum": "4B",
          "city": "Seattle",
          "state": "WA",
          "zip": "98101",
          "country": "US"
        },
        "description": "Apartment designation with alphanumeric unit number"
      }
    ],
    "facilities": [
      {
        "input": "Fenway Park – 4 Jersey Street, Boston MA 02215",
        "expected": {
          "place": "Fenway Park",
          "number": "4",
          "street": "Jersey",
          "type": "St",
          "city": "Boston",
          "state": "MA",
          "zip": "02215"
        },
        "description": "Fenway Park with em dash separator"
      },
      {
        "input": "Harvard University, Massachusetts Hall, Cambridge MA 02138",
        "expected": {
          "place": "Harvard University",
          "street": "Massachusetts Hall",
          "city": "Cambridge",
          "state": "MA",
          "zip": "02138"
        },
        "description": "Harvard University with building name as street"
      },
      {
        "input": "Golden Gate Bridge Welcome Center\nGolden Gate Bridge Hwy 101, San Francisco CA 94129",
        "expected": {
          "place": "Golden Gate Bridge Welcome Center",
          "street": "Golden Gate Bridge Hwy 101",
          "city": "San Francisco",
          "state": "CA",
          "zip": "94129"
        },
        "description": "Bridge visitor center with newline and highway designation"
      },
      {
        "input": "Lincoln Memorial - 2 Lincoln Memorial Circle NW, Washington DC 20037",
        "expected": {
          "place": "Lincoln Memorial",
          "number": "2",
          "street": "Lincoln Memorial",
          "type": "Cir",
          "suffix": "NW",
          "city": "Washington",
          "state": "DC",
          "zip": "20037"
        },
        "description": "Memorial with dash separator and directional suffix"
      },
      {
        "input": "The Pentagon: 1400 Defense Pentagon, Washington DC 20301",
        "expected": {
          "place": "The Pentagon",
          "number": "1400",
          "street": "Defense Pentagon",
          "city": "Washington",
          "state": "DC",
          "zip": "20301"
        },
        "description": "Pentagon with colon separator and military address format"
      },
      {
        "input": "The White House\n1600 Pennsylvania Avenue NW, Washington DC 20500",
        "expected": {
          "place": "The White House",
          "number": "1600",
          "street": "Pennsylvania",
          "type": "Ave",
          "suffix": "NW",
          "city": "Washington",
          "state": "DC",
          "zip": "20500"
        },
        "description": "White House with newline separator"
      },
      {
        "input": "Walt Disney World Resort, 1375 E Buena Vista Drive, Lake Buena Vista FL 32830",
        "expected": {
          "place": "Walt Disney World Resort",
          "number": "1375",
          "prefix": "E",
          "street": "Buena Vista",
          "type": "Dr",
          "city": "Lake Buena Vista",
          "state": "FL",
          "zip": "32830"
        },
        "description": "Theme park resort with comma separators"
      },
      {
        "input": "Willis Tower\n233 S Wacker Drive\nChicago IL 60606",
        "expected": {
          "place": "Willis Tower",
          "number": "233",
          "prefix": "S",
          "street": "Wacker",
          "type": "Dr",
          "city": "Chicago",
          "state": "IL",
          "zip": "60606"
        },
        "description": "Skyscraper with newline-formatted address"
      },
      {
        "input": "Madison Square Garden | 4 Pennsylvania Plaza, New York NY 10001",
        "expected": {
          "place": "Madison Square Garden",
          "number": "4",
          "street": "Pennsylvania",
          "type": "Plz",
          "city": "New York",
          "state": "NY",
          "zip": "10001"
        },
        "description": "Arena with pipe separator and Plaza type"
      },
      {
        "input": "Space Needle; 400 Broad Street, Seattle WA 98109",
        "expected": {
          "place": "Space Needle",
          "number": "400",
          "street": "Broad",
          "type": "St",
          "city": "Seattle",
          "state": "WA",
          "zip": "98109"
        },
        "description": "Landmark with semicolon separator"
      },
      {
        "input": "Mount Rushmore National Memorial\n13000 SD‑244\nKeystone SD 57751",
        "expected": {
          "place": "Mount Rushmore National Memorial",
          "street": "SD‑244",
          "city": "Keystone",
          "state": "SD",
          "zip": "57751"
        },
        "description": "Memorial with state highway en-dash designation"
      }
    ]
  }
}