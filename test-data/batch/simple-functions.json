{
  "$schema": "../../schemas/test-file.json",
  "name": "Simple Functions Batch Testing",
  "description": "Test data for simple batch functions (parseLocations, parseAddresses, etc.)",
  "tests": {
    "parseLocations": [
      {
        "description": "Basic address parsing functionality",
        "addresses": [
          "123 Main St, New York, NY 10001",
          "456 Oak Ave, Los Angeles, CA 90210",
          "789 Pine Rd, Chicago, IL 60601"
        ],
        "expectedResults": [
          {
            "number": "123",
            "street": "Main",
            "type": "St",
            "city": "New York",
            "state": "NY",
            "zip": "10001"
          },
          {
            "number": "456",
            "street": "Oak",
            "type": "Ave",
            "city": "Los Angeles",
            "state": "CA",
            "zip": "90210"
          },
          {
            "number": "789",
            "street": "Pine",
            "type": "Rd",
            "city": "Chicago",
            "state": "IL",
            "zip": "60601"
          }
        ]
      },
      {
        "description": "Handling invalid addresses",
        "addresses": [
          "123 Main St, New York, NY 10001",
          "",
          "456 Oak Ave, Los Angeles, CA 90210"
        ],
        "expectedSuccessCount": 2,
        "expectedFailureCount": 1
      },
      {
        "description": "Empty input array",
        "addresses": [],
        "expectedSuccessCount": 0,
        "expectedFailureCount": 0
      }
    ],
    "parseIntersections": [
      {
        "description": "Basic intersection parsing",
        "addresses": [
          "Main Street & Oak Avenue, New York, NY",
          "Broadway and 42nd Street, New York, NY"
        ],
        "expectedResults": [
          {
            "street1": "Main",
            "type1": "St",
            "street2": "Oak",
            "type2": "Ave",
            "city": "New York",
            "state": "NY"
          },
          {
            "street1": "Broadway",
            "type1": "",
            "street2": "42nd",
            "type2": "St",
            "city": "New York",
            "state": "NY"
          }
        ]
      }
    ],
    "parseInformalAddresses": [
      {
        "description": "Addresses with secondary units",
        "addresses": [
          "123 Main St, Apt 4B, New York, NY 10001",
          "456 Oak Ave, Suite 200, Los Angeles, CA 90210"
        ],
        "expectedSuccessCount": 2,
        "expectedFailureCount": 0
      }
    ],
    "options": [
      {
        "description": "Testing useSnakeCase option",
        "addresses": [
          "123 Main St, New York, NY 10001",
          "456 Oak Ave, Los Angeles, CA 90210"
        ],
        "options": {
          "useSnakeCase": true
        },
        "expectedSuccessCount": 2,
        "expectedFailureCount": 0
      },
      {
        "description": "Testing country-specific options for Canada",
        "addresses": [
          "123 Yonge Street, Toronto, ON M5B 1M4",
          "456 Granville Street, Vancouver, BC V6C 1V5"
        ],
        "options": {
          "country": "CA"
        },
        "expectedSuccessCount": 2,
        "expectedFailureCount": 0
      }
    ]
  }
}